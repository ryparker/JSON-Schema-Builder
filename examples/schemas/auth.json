{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "maxLength": 8,
      "minLength": 8
    },
    "roles": {
      "type": "array",
      "items": {
        "type": "string",
        "maxLength": 13,
        "minLength": 13
      }
    },
    "access_token": {
      "type": "string",
      "maxLength": 32,
      "minLength": 32
    },
    "token_type": {
      "type": "string",
      "maxLength": 6,
      "minLength": 6
    },
    "token_age": {
      "type": "number",
      "maximum": 12,
      "minimum": 12
    }
  },
  "required": [
    "access_token",
    "roles",
    "token_age",
    "token_type",
    "username"
  ],
  "additionalProperties": false
}