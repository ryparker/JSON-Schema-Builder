{
  "name": "json-schema-builder",
  "version": "0.9.0",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": "https://ryparker@github.com/ryparker/JSON-Schema-Builder.git",
  "author": "Ryan Parker",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn tsc",
    "lint": "xo src",
    "fix": "xo src --fix",
    "test": "yarn build && yarn ava --verbose",
    "clean": "rimraf ./dist"
  },
  "ava": {
    "failFast": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "exclude": [
      "dist/**",
      "docs/**"
    ]
  },
  "xo": {
    "prettier": true,
    "rules": {
      "@typescript-eslint/no-var-requires": 0
    }
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/lodash": "^4.14.155",
    "@types/node": "^14.0.13",
    "ajv": "^6.12.2",
    "ava": "^3.9.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "xo": "^0.32.0"
  },
  "keywords": [
    "json schema",
    "json",
    "schema",
    "builder",
    "genson",
    "generator",
    "trainer",
    "node"
  ]
}
